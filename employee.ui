     <property name="styleSheet">
      <string notr="true">/* Styles for QComboBox */
QComboBox {
    border: 2px solid #141E46; /* Border color */
    border-radius: 10px; /* Border radius */
    padding: 5px; /* Padding */
    min-width: 100px; /* Minimum width */
    selection-background-color: #EBF1F4; /* Background color when item is selected */
    background-color: white; /* Default background color */
    color: black; /* Text color */
}

QComboBox:hover {
    border-color: #EBF1F4; /* Border color on hover */
}

QComboBox::drop-down {
    subcontrol-origin: padding; /* Positioning origin of the drop-down arrow */
    subcontrol-position: right; /* Position of the drop-down arrow */
    width: 20px; /* Width of the drop-down arrow */
    border-left: 2px solid #141E46; /* Left border color of the drop-down arrow */
}

QComboBox::down-arrow {
    image: url(down_arrow.png); /* Image for the drop-down arrow */
}

/* Styles for QComboBox items */
QComboBox::item {
    padding: 5px; /* Padding for each item */
}

QComboBox::item:selected {
    background-color: #EBF1F4; /* Background color for selected item */
    color: black; /* Text color for selected item */
}
</string>
