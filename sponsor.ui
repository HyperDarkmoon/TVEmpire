}

QComboBox::drop-down {
    subcontrol-origin: padding; /* Positioning origin of the drop-down arrow */
    subcontrol-position: right; /* Position of the drop-down arrow */
    width: 20px; /* Width of the drop-down arrow */
    border-left: 2px solid #141E46; /* Left border color of the drop-down arrow */
}

QComboBox::down-arrow {
